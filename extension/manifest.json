{
  "manifest_version": 3,
  "name": "mp3by4",
  "version": "1.0",
  "description": "An extension to convert text into explanatory videos or fun songs.",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
